{
  "name": "Vela Spectrum Light (tritanopia)",
  "type": "vs",
  "colors": {
    "editor.background": "#000000",
    "editor.foreground": "#000000",
    "editorCursor.foreground": "#00020f",
    "focusBorder": "#000312cc",
    "activityBar.background": "#000000",
    "activityBar.foreground": "#00020f",
    "activityBar.activeBackground": "#00020e99",
    "activityBar.activeBorder": "#00020f",
    "button.background": "#00020e99",
    "button.hoverBackground": "#000312cc",
    "badge.background": "#00020f",
    "badge.foreground": "#ffffff",
    "list.activeSelectionBackground": "#00020e99",
    "terminal.ansiYellow": "#001200",
    "terminal.ansiGreen": "#000400",
    "terminal.ansiRed": "#130000",
    "terminal.ansiBlue": "#160046",
    "scrollbarSlider.background": "#0000001a",
    "scrollbarSlider.hoverBackground": "#00000066",
    "minimap.background": "#000000",
    "minimapSlider.background": "#0000001a"
  },
  "tokenColors": [
    {
      "scope": [
        "comment",
        "string.comment"
      ],
      "settings": {
        "foreground": "#00000066",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#000400"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#00020f"
      }
    },
    {
      "scope": "support.function",
      "settings": {
        "foreground": "#00020f"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#000000"
      }
    }
  ]
}